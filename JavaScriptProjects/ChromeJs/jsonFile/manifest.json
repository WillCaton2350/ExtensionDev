{
    "manifest_version": 2,
    "name": "Volume Control Extension",
    "version": "1.0",
    "permissions": [
      "activeTab"
    ],
    "browser_action": {
      "default_popup": "index.html"
    },
    "icons": {
      "48": "icon.png"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["script.js"]
      }
    ]
  }